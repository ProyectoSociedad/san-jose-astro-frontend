{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "typeRoots": ["./node_modules/@types", "./types"],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "include": ["src", "types/**/*"]
}
